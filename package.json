{
  "name": "falling-text",
  "scripts": {
    "build": "babel src -d dist; cp src/*.html dist; cp src/*.css dist"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.8"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "> 1%",
              "last 2 versions"
            ]
          }
        }
      ]
    ],
    "compact": true,
    "comments": false
  }
}
